{
  "name": "spiderScore",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Парсер MyScore.com",
  "permissions": [
     "<all_urls>"
   ],
  "app": {
      "launch": {
         "local_path": "main.html"
      }
   },
   "icons": { "48": "Spider-48.png" },
   "web_accessible_resources" : [ "js/jquery-1.7.2.min.js", "js/main.js" ],
   "content_scripts": [{"matches": [ "<all_urls>" ], "js": [ "js/jquery-1.7.2.min.js", "js/main.js" ] }]
}

